<?xml version='1.0' standalone='yes'?>
<bindepot default-path="./files">
	<store name="images" type="image" >
		<format name="small" width="80" height="80" fit="crop" />
		<format name="smallpng" width="80" height="80" fit="crop" type="png"/>
		<format name="smallorg" width="80" height="80" fit="crop" type="original"/>
		<format width="150" height="120" fit="crop" />
		<format name="hstrip" max-height="100" fit="crop" />
		<format name="maxmin" max-height="100" max-width="150" fit="crop" />
		<format name="grayscale" width="150" height="120" filter="grayscale" fit="crop" />
		<format name="mean_removal" width="150" height="120" filter="mean_removal" fit="crop" />
	</store>
	<store name="docs" type="binary" >
	</store>
</bindepot>
